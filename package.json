{
  "name": "react-vite-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,red\"",
    "dev:frontend": "vite --host 0.0.0.0",
    "dev:backend": "cd backend && bun run dev",
    "prebuild": "node scripts/generate-sitemap.js",
    "build": "bunx vite build --outDir dist",
    "build:backend": "cd backend && bun install",
    "lint": "bunx tsc --noEmit && bunx biome lint --write",
    "format": "bunx biome format --write",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "prepare": "husky install",
    "db:setup": "cd backend && bun run db:setup"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bunx biome lint --write",
      "bunx biome format --write"
    ]
  },
